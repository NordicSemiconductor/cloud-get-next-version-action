{
  "name": "@nordicsemiconductor/get-next-version-action",
  "version": "0.0.0-development",
  "description": "Determines the next release version for this repository",
  "repository": {
    "type": "git",
    "url": "https://github.com/NordicSemiconductor/cloud-get-next-version-action.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-get-next-version-action/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-get-next-version-action#readme",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@semantic-release/commit-analyzer": "9.0.1",
    "semantic-release": "18.0.0",
    "stream-buffers": "3.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-angular": "13.2.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.1.0",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "8.1.8",
    "@types/node": "14.17.22",
    "@types/semantic-release": "17.2.2",
    "@types/stream-buffers": "3.0.4",
    "@vercel/ncc": "0.31.1",
    "eslint": "8.0.0",
    "husky": "7.0.2",
    "lint-staged": "11.2.3",
    "pinst": "2.1.6",
    "prettier": "2.4.1",
    "typescript": "4.4.3"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": "8.0.0"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  }
}
