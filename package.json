{
  "name": "@bifravst/get-next-version-action",
  "version": "0.0.0-development",
  "description": "Determines the next release version for this repository",
  "scripts": {
    "build": "npx ncc build get-next-version.ts -o build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bifravst/get-next-version-action.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/get-next-version-action/issues"
  },
  "homepage": "https://bifravst.github.io/",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "Asset Tracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-5-Clause-Nordic",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "semantic-release": "^17.3.6",
    "stream-buffers": "^3.0.2"
  },
  "devDependencies": {
    "@bifravst/code-style": "8.0.134",
    "@bifravst/eslint-config-typescript": "^5.0.89",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@types/node": "14.14.22",
    "@types/semantic-release": "^17.2.0",
    "@types/stream-buffers": "^3.0.3",
    "@vercel/ncc": "0.27.0",
    "eslint": "7.18.0",
    "husky": "^4.3.8",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run build && git add -A build"
    }
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.js": [
      "prettier --write",
      "eslint --ext .js"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
