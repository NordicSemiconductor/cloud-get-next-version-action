{
  "name": "@bifravst/firmware",
  "version": "0.0.0-development",
  "description": "Cat Tracker application built using nRF Connect SDK",
  "repository": {
    "type": "git",
    "url": "https://github.com/bifravst/firmware.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/firmware/issues"
  },
  "homepage": "https://bifravst.github.io/",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "AssetTracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-5-Clause-Nordic",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "husky": "^4.2.1",
    "semantic-release": "^17.0.2",
    "standard": "^14.3.1",
    "stream-buffers": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e"
    }
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "/home/runner/work/cat-tracker-fw/cat-tracker-fw/nrf/cat-tracker-nrf9160_pca10015ns.hex",
              "name": "cat-tracker-PCA10015-AssetTracker-${nextRelease.gitTag}.hex",
              "label": "Pre-build HEX file for PCA10015 / Asset Tracker (${nextRelease.gitTag})"
            },
            {
              "path": "/home/runner/work/cat-tracker-fw/cat-tracker-fw/nrf/cat-tracker-nrf9160_pca20035ns.hex",
              "name": "cat-tracker-PCA20035-Thingy52-${nextRelease.gitTag}.hex",
              "label": "Pre-build HEX file for PCA20035 / Thingy:91 (${nextRelease.gitTag})"
            },
            {
              "path": "/home/runner/work/cat-tracker-fw/cat-tracker-fw/nrf/cat-tracker-nrf9160_pca10090ns.hex",
              "name": "cat-tracker-PCA10090-nRF9160DK-${nextRelease.gitTag}.hex",
              "label": "Pre-build HEX file for PCA10090 / nRF9160 DK (${nextRelease.gitTag})"
            }
          ]
        }
      ]
    ]
  }
}