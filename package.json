{
  "name": "@nordicsemiconductor/get-next-version-action",
  "version": "0.0.0-development",
  "description": "Determines the next release version for this repository",
  "repository": {
    "type": "git",
    "url": "https://github.com/NordicSemiconductor/cloud-get-next-version-action.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-get-next-version-action/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-get-next-version-action#readme",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@semantic-release/commit-analyzer": "8.0.1",
    "semantic-release": "17.4.2",
    "stream-buffers": "3.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-angular": "12.1.1",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.0.2",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "7.0.17",
    "@types/node": "14.14.41",
    "@types/semantic-release": "17.2.0",
    "@types/stream-buffers": "3.0.3",
    "@vercel/ncc": "0.28.3",
    "eslint": "7.24.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "pinst": "2.1.6",
    "prettier": "2.2.1",
    "typescript": "4.2.4"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": "7.11.0"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  }
}
